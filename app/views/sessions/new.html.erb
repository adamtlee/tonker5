<h1>Sessions new page</h1>

<%= form_for(:session, url: login_path) do |form| %> 
    <div>
        <%= form.label :username, "Username" %>
        <%= form.text_field :username, placeholder: "Username" %>
    </div>
    <div>
        <%= form.label :password, "Password" %>
        <%= form.text_field :password, placeholder: "Password" %>
    </div>
    <%= form.button "login" %>
<% end %>